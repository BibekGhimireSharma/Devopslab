import java.util.*;

class Solution {
    public String[] spellchecker(String[] wordlist, String[] queries) {
        Set<String> exact = new HashSet<>();
        Map<String, String> lowerMap = new HashMap<>();
        Map<String, String> vowelMap = new HashMap<>();
        
        for (String w : wordlist) {
            exact.add(w);
            String lower = w.toLowerCase();
            lowerMap.putIfAbsent(lower, w);
            String dev = devowel(lower);
            vowelMap.putIfAbsent(dev, w);
        }
        
        String[] ans = new String[queries.length];
        for (int i = 0; i < queries.length; ++i) {
            String q = queries[i];
            if (exact.contains(q)) {
                ans[i] = q; // exact case-sensitive match
                continue;
            }
            String qLower = q.toLowerCase();
            if (lowerMap.containsKey(qLower)) {
                ans[i] = lowerMap.get(qLower); // first case-insensitive match
                continue;
            }
            String qDev = devowel(qLower);
            if (vowelMap.containsKey(qDev)) {
                ans[i] = vowelMap.get(qDev); // first vowel-error match
                continue;
            }
            ans[i] = ""; // no match
        }
        return ans;
    }
    
    private static String devowel(String s) {
        StringBuilder sb = new StringBuilder(s.length());
        for (char c : s.toCharArray()) {
            if (isVowel(c)) sb.append('*');
            else sb.append(c);
        }
        return sb.toString();
    }
    
    private static boolean isVowel(char c) {
        return c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u';
    }
}
